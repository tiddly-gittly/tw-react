{
  "description": "npm scripts to build plugins",
  "license": "MIT",
  "name": "tw-react",
  "version": "0.1.4",
  "url": "https://github.com/tiddly-gittly/tw-react",
  "author": "Lin Onetwo",
  "types": "dist/lib/widget.d.ts",
  "files": [
    "dist/output/*.json",
    "dist/lib/widget.d.ts"
  ],
  "scripts": {
    "make": "npm run clean && npm run download-react && npm run build",
    "dev": "npm run clean && npm run build && npm run run-action",
    "clean": "rm -rf ./dist",
    "download-react": "zx scripts/download-react.mjs",
    "build": "rollup -c && zx scripts/after-build.mjs",
    "run-action": "rollup -c && zx scripts/run-action.mjs",
    "prepublishOnly": "npm run make && tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/pinyin": "^2.10.0",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "archiver": "^5.3.0",
    "github-action-ts-run-api": "^2.0.0",
    "rollup": "^2.70.1",
    "rollup-plugin-disable-packages": "^4.0.1",
    "tiddlywiki": "^5.2.1",
    "tslib": "^2.3.1",
    "tw5-plugin-packer": "^0.0.8",
    "tw5-typed": "^0.1.12",
    "typescript": "^4.6.2",
    "zx": "^6.0.7"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
