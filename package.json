{
  "description": "npm scripts to build plugins",
  "tsFiles": [
    "widget",
    "example",
    "index"
  ],
  "license": "MIT",
  "name": "tw-react",
  "version": "0.3.2",
  "url": "https://github.com/tiddly-gittly/tw-react",
  "author": "Lin Onetwo",
  "types": "dist/lib/index.d.ts",
  "files": [
    "dist/output/*.json",
    "dist/plugins/linonetwo/tw-react/**/*.js",
    "dist/lib/**/*.d.ts"
  ],
  "main": "dist/plugins/linonetwo/tw-react/index.js",
  "scripts": {
    "make": "npm run clean && npm run build && npm run download-react",
    "dev-demo": "npm run dev && zx ./scripts/build-demo-html.mjs && cd ./dist/output && serve .",
    "dev": "npm run make && npm run run-action && zx ./scripts/mv-dev.mjs",
    "clean": "rm -rf ./dist",
    "build": "zx esbuild.config.mjs && zx scripts/after-build.mjs && zx esbuild.react_jsx-runtime.config.mjs",
    "run-action": "zx scripts/run-action.mjs",
    "prepublishOnly": "npm run make && tsc --emitDeclarationOnly",
    "download-react": "zx scripts/download-react.mjs",
    "installType": "typesync"
  },
  "devDependencies": {
    "@types/archiver": "5.3.1",
    "@types/eslint": "8.4.1",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/fs-extra": "9.0.13",
    "@types/prettier": "2.6.0",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "archiver": "5.3.1",
    "browserslist": "4.20.2",
    "esbuild": "0.14.38",
    "esbuild-plugin-browserslist": "0.4.9",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-autofix": "1.1.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "eslint-plugin-security": "1.5.0",
    "eslint-plugin-security-node": "1.1.1",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unicorn": "42.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "fs-extra": "10.1.0",
    "github-action-ts-run-api": "2.1.0",
    "prettier": "2.6.2",
    "serve": "13.0.2",
    "tiddlywiki": "5.2.2",
    "tslib": "2.4.0",
    "tw5-plugin-packer": "0.0.10",
    "tw5-typed": "0.2.3",
    "typescript": "4.6.3",
    "typesync": "0.9.0",
    "react": "^18.0.0",
    "zx": "6.1.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
