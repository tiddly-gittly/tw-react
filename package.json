{
  "description": "npm scripts to build plugins",
  "license": "MIT",
  "name": "tw-react",
  "version": "0.0.10",
  "url": "https://github.com/tiddly-gittly/tw-react",
  "author": "Lin Onetwo",
  "types": "dist/lib/widget.d.ts",
  "files": [
    "dist/output/*.json",
    "dist/lib/widget.d.ts"
  ],
  "scripts": {
    "make": "npm run clean && npm run download-react && npm run build && npm run buildJSON",
    "dev": "npm run build && npm run buildJSON",
    "clean": "rm -rf ./dist",
    "download-react": "zx scripts/download-react.mjs",
    "buildJSON": "zx scripts/buildJSONPlugins.mjs",
    "zip": "zx scripts/zipDist.mjs",
    "build": "rollup -c && zx scripts/after-build.mjs",
    "prepublishOnly": "npm run make && tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/pinyin": "^2.10.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "rollup": "^2.67.0",
    "rollup-plugin-disable-packages": "^4.0.1",
    "tiddlywiki": "^5.2.1",
    "tslib": "^2.3.1",
    "tw5-typed": "^0.1.11",
    "typescript": "^4.5.5",
    "zx": "^4.3.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
